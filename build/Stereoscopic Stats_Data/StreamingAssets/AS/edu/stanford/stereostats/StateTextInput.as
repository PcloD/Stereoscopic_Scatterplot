package edu.stanford.stereostats {		import flash.display.MovieClip;	import scaleform.clik.controls.TextArea;	import scaleform.clik.events.InputEvent;    import flash.events.Event;    import flash.events.FocusEvent;    import flash.events.KeyboardEvent;    import flash.events.MouseEvent;		public class StateTextInput extends TextArea {				public function StateTextInput() {			super();		}				public function _handleMouseDown(e:MouseEvent):void {			handleMouseDown(e);        }		public function _handleRollOut(e:MouseEvent):void {			handleRollOut(e);        }		public function _handleRollOver(e:MouseEvent):void {			handleRollOver(e);        }		public function _handleInput(e:InputEvent):void {			trace(e.target["text"], e.currentTarget["text"]);			handleInput(e);        }		public function _handleTextFieldFocusIn(e:FocusEvent):void {			handleTextFieldFocusIn(e);        }	}	}