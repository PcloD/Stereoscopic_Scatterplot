package edu.stanford.stereostats {		import scaleform.clik.controls.Button;	import flash.events.Event;	import flash.events.MouseEvent;    import scaleform.clik.events.InputEvent;		public class StateButton extends Button {				public function StateButton() {			super();		}				public function _handleMouseRollOver(e:MouseEvent):void {			handleMouseRollOver(e);        }		public function _handleMouseRollOut(e:MouseEvent):void {			handleMouseRollOut(e);        }		public function _handleMousePress(e:MouseEvent):void {			handleMousePress(e);        }		public function _handleMouseRelease(e:MouseEvent):void {			handleMouseRelease(e);        }		public function _handleInput(e:InputEvent):void {			handleInput(e);        }	}	}